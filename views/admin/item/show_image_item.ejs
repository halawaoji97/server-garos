<div class="tab-pane fade show active" id="pills-show-image-item" role="tabpanel"
    aria-labelledby="pills-show-image-item-tab">
    <!-- DataTales Example -->
    <div class="row">
        <% for(let x=0; x < item.imageId.length; x++){ %>
            <div class="col-md-4">
                <div class="card shadow mb-4 mt-2">
                    <div class="card-body">
                        <img src="http://localhost:3000/<%= item.imageId[x].imageUrl %>" alt="image"
                            style="width: 100%; display: block;">
                    </div>
                </div>
            </div>
            <% } %>
    </div>
    <a href="/admin/item" class="btn btn-warning">Back</a>
</div>